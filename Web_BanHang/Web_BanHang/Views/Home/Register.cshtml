
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <!-- Meta Tag -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name='copyright' content=''>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Title Tag  -->
    <title>Eshop - eCommerce HTML5 Template.</title>
    <!-- Favicon -->
    <link rel="icon" type="image/png" href=@Url.Content("/Content/images/favicon.png")>
    <!-- Web Font -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">

    <!-- StyleSheet -->
    <!-- Bootstrap -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/bootstrap.css")>
    <!-- Magnific Popup -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/magnific-popup.min.css")>
    <!-- Font Awesome -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/font-awesome.css")>
    <!-- Fancybox -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/jquery.fancybox.min.css")>
    <!-- Themify Icons -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/themify-icons.css")>
    <!-- Nice Select CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/niceselect.css")>
    <!-- Animate CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/animate.css")>
    <!-- Flex Slider CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/flex-slider.min.css")>
    <!-- Owl Carousel -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/owl-carousel.css")>
    <!-- Slicknav -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/slicknav.min.css")>

    <!-- Eshop StyleSheet -->
    <link rel="stylesheet" href=@Url.Content("/Content/css/reset.css")>
    <link rel="stylesheet" href=@Url.Content("/Content/style.css")>
    <link rel="stylesheet" href=@Url.Content("/Content/css/responsive.css")>
</head>
<body class="js bg-light">
    <section id="contact-us" class="contact-us section">
        <div class="container">
            <div class="contact-head">
                <div class="row justify-content-center">
                    <div class="col-lg-6 col-12">
                        <div class="form-main bg-white">
                            <div class="title text-center">
                                <h3>ĐĂNG KÝ</h3>
                            </div>
                            <form class="form" method="post" action="">
                                <div class="row">
                                    <div class="col-lg-12 col-12">
                                        <div class="form-group">
                                            <label>Email<span>*</span></label>
                                            <input name="email" type="text" placeholder="">
                                        </div>
                                    </div>
                                    <div class="col-lg-12 col-12">
                                        <div class="form-group">
                                            <label>Mật khẩu<span>*</span></label>
                                            <input name="password" type="password" placeholder="">
                                        </div>
                                    </div>
                                    <div class="col-lg-12 col-12">
                                        <div class="form-group">
                                            <label>Nhập lại mật khẩu<span>*</span></label>
                                            <input name="password" type="password" placeholder="">
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-group row justify-content-center">
                                            <button type="submit" class="btn">Đăng kí</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Jquery -->
    <script src=@Url.Content("/Content/js/jquery.min.js")></script>
    <script src=@Url.Content("/Content/js/jquery-migrate-3.0.0.js")></script>
    <script src=@Url.Content("/Content/js/jquery-ui.min.js")></script>
    <!-- Popper JS -->
    <script src=@Url.Content("/Content/js/popper.min.js")></script>
    <!-- Bootstrap JS -->
    <script src=@Url.Content("/Content/js/bootstrap.min.js")></script>
    <!-- Color JS -->
    <script src=@Url.Content("/Content/js/colors.js")></script>
    <!-- Slicknav JS -->
    <script src=@Url.Content("/Content/js/slicknav.min.js")></script>
    <!-- Owl Carousel JS -->
    <script src=@Url.Content("/Content/js/owl-carousel.js")></script>
    <!-- Magnific Popup JS -->
    <script src=@Url.Content("/Content/js/magnific-popup.js")></script>
    <!-- Fancybox JS -->
    <script src=@Url.Content("/Content/js/facnybox.min.js")></script>
    <!-- Waypoints JS -->
    <script src=@Url.Content("/Content/js/waypoints.min.js")></script>
    <!-- Countdown JS -->
    <script src=@Url.Content("/Content/js/finalcountdown.min.js")></script>
    <!-- Nice Select JS -->
    <script src=@Url.Content("/Content/js/nicesellect.js")></script>
    <!-- Ytplayer JS -->
    <script src=@Url.Content("/Content/js/ytplayer.min.js")></script>
    <!-- Flex Slider JS -->
    <script src=@Url.Content("/Content/js/flex-slider.js")></script>
    <!-- ScrollUp JS -->
    <script src=@Url.Content("/Content/js/scrollup.js")></script>
    <!-- Onepage Nav JS -->
    <script src=@Url.Content("/Content/js/onepage-nav.min.js")></script>
    <!-- Easing JS -->
    <script src=@Url.Content("/Content/js/easing.js")></script>
    <!-- Active JS -->
    <script src=@Url.Content("/Content/js/active.js")></script>
</body>
</html>
